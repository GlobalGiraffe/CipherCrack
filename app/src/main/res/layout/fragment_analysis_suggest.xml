<?xml version="1.0" encoding="utf-8"?>
<FrameLayout tools:context="mnh.game.ciphercrack.AnalysisActivity"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:theme="@style/CrackTheme"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:id="@+id/static_suggest_layout"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:background="@color/black_overlay"
        tools:ignore="UselessParent"
        android:padding="6sp"
        android:orientation="vertical"
        android:layout_gravity="center">
        <TextView
            android:id="@+id/stats_suggest_text"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="@drawable/result_text_border"
            android:padding="10sp"
            android:text="@string/suggest"
            android:textColor="@color/entry_text_text"
            android:textIsSelectable="true"
            android:textSize="16sp" />
    </LinearLayout>
</FrameLayout>